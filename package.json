{
	"name": "habit-tracker",
	"version": "0.2.1",
	"description": "Track your Habits.",
	"main": "main.js",
	"scripts": {
		"dev": "concurrently \"node esbuild.config.mjs\" \"npm run copyAndWatchMainJS\" \"npm run copyAndWatchStylesJS\" \"npm run copyAndWatchManifestJS\"",
		"copyAndWatchMainJS": "cpx \"main.js\" \"Example\\.obsidian\\plugins\\heatmap-calendar\" -w",
		"copyAndWatchStylesJS": "cpx \"styles.css\" \"Example\\.obsidian\\plugins\\heatmap-calendar\" -w",
		"copyAndWatchManifestJS": "cpx \"manifest.json\" \"Example\\.obsidian\\plugins\\heatmap-calendar\" -w",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && rollup main.js --format cjs --file build/main.js",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest"
	},
	"keywords": [
		"obsidian"
	],
	"author": "David Moeller",
	"license": "Apache-2.0 License",
	"devDependencies": {
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/jest": "^28.1.4",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"builtin-modules": "^3.2.0",
		"concurrently": "^7.2.1",
		"cpx": "^1.5.0",
		"esbuild": "0.13.12",
		"obsidian": "^0.12.17",
		"rollup": "^2.75.7",
		"rollup-plugin-dts": "^4.2.2",
		"ts-jest": "^28.0.5",
		"tslib": "2.3.1",
		"typescript": "4.4.4"
	},
	"dependencies": {
		"date-fns": "^2.28.0"
	}
}
